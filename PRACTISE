let bill = 100;
let premium = "Yes"
let discountedBill = 0;

if(bill > 50 && bill <100 && premium!="Yes"){
    discountedBill = bill*0.1; 
}
else if(bill > 50 && bill <100 && premium == "Yes"){
    discountedBill = bill*0.15;
}
else if( bill >= 100  && premium!="Yes"){
    discountedBill = bill*0.2;
}
else if( bill >= 100 && premium == "Yes"){
    discountedBill = bill*0.25;
}

document.write(discountedBill);
  </script>
