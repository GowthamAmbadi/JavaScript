<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <script>

    // 1)for loop :
    //  for (initialisation; condition ,update-expression{
    // ---block of code
    // }

    // print 1 to 5 numbers
    for(let i=1;i<=5;i++){
      document.write(i+"<br>")
    }
    // inti : i = 1,2,3,4,5
    // condi : 1<=5 t , 2<=5 t , 3<=5 t , 4<=5 t , 5<=5 t , 6<=5 f
    // up : i++ =i=i+1 1+1=2 , 2+1=3 , 4,5,6
    // block of code : 1 ,2 , 3, 4, 5

    document.write("<br>")

    for(let j=0;j<7;j=j+2){
      document.write(j+2+"<br>")
    }
    // Initialisation: j = 0
    // Condition:0 < 7 (true) , 2 < 7 (true) , 4 < 7 (true) , 6 < 7 (true) , 8 < 7 (false)
    // Update: j = j + 2 , 0 + 2 = 2 , 2 + 2 = 4 , 4 + 2 = 6 , 6 + 2 = 8
    // Block of code: 2,4,6,8
    document.write("<br>")

    // print numbers from 10 to 15
    for(let k=10;k<=15;k++){
      document.write(k+"<br>")
    }
    document.write("<br>")

    // print 'gowtham' 5 times
    for(let l=0;l<5;l++){
      document.write("gowtham"+"<br>")
    }
    document.write("<br>")
    
    //print squares of numbers from 1 to 10
    for(let m=1;m<=10;m++){
      document.write(m*m+"<br>")
    }
    document.write("<br>")

    //print 1 4 9 16 25 ....... 100
    for(let n=1;n<=10;n++){
      document.write(n*n*n+"<br>")
    }
    document.write("<br>")

    //print 9th table
    //for(let n=1;n<=10;n++){
    //document.write(9*n+"<br>")
    //}
    //document.write("<br>")

    // print 9 table
    for(let p=1;p<=10;p++){
      document.write("9 x "+p+" = "+(9*p)+"<br>")
    }
    document.write("<br>")

    const numbers = [1,2,3,4,5]
    let sum = 0
    for (let q=0;q<numbers.length;q++){
      //0<5;1<5;2<5;3<5;4<5;5<5 = F
      sum = sum + numbers[q]
      // 0+1=1 [0=1]
      // 1+2=3 [1=3]
      // 3+3=6 [2=6]
      // 6+4=10 [3=10]
      // 10+5=15 [4=15]
      document.write(sum+"<br>")
    }
    
  </script>
</body>
</html>
